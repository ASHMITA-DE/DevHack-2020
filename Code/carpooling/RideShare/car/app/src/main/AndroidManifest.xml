<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.car">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="com.example.carP.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.carP.MainActivity" />
        <activity android:name="com.example.carP.SignUpActivity" />
        <activity android:name="com.example.carP.OfferActivity" />
        <activity android:name="com.example.carP.SearchActivity" />
        <activity android:name="com.example.carP.UpdateActivity" />
        <activity android:name="com.example.carP.RideConfirmation" />
        <service android:name="com.example.carP.common.FetchAddress"/>
    </application>

</manifest>